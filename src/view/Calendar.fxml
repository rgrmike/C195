      <Button fx:id="CalendarEditButton" layoutX="365.0" layoutY="329.0" mnemonicParsing="false" onAction="#CalendarEditButtonHandler" text="Edit" />
      <Button fx:id="CalendarNewButton" layoutX="310.0" layoutY="329.0" mnemonicParsing="false" onAction="#CalendarNewButtonHandler" text="New" />
      <Label layoutX="294.0" layoutY="73.0" text="View Appointments by:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="CalendarCustomersButton" layoutX="23.0" layoutY="329.0" mnemonicParsing="false" onAction="#CalendarCustomersButtonHandler" text="Customers" />
      <Button fx:id="CalendarsReportsButton" layoutX="106.0" layoutY="329.0" mnemonicParsing="false" onAction="#CalendarsReportsButtonHandler" text="Reports" />
      <Button fx:id="CalendarAllButton" layoutX="620.0" layoutY="70.0" mnemonicParsing="false" onAction="#CalendarAllButtonHandler" text="All" />
