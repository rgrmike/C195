<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="368.0" prefWidth="674.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.CalendarController">
   <children>
      <TableView fx:id="CalendarTable" layoutX="23.0" layoutY="107.0" prefHeight="213.0" prefWidth="629.0">
        <columns>
          <TableColumn fx:id="CalendarApptCol" prefWidth="105.0" text="Appointment" />
          <TableColumn fx:id="CalendarStartCol" prefWidth="84.0" text="Start Time" />
            <TableColumn fx:id="CalendarEndCol" prefWidth="75.0" text="End Time" />
            <TableColumn fx:id="CalendarContactCol" prefWidth="75.0" text="Contact" />
            <TableColumn fx:id="CalendarCustCol" prefWidth="75.0" text="Customer" />
            <TableColumn fx:id="CalendarDescCol" prefWidth="139.0" text="Description" />
            <TableColumn fx:id="CalendarApptID" prefWidth="75.0" text="ApptID" />
        </columns>
      </TableView>
      <Label layoutX="14.0" layoutY="14.0" text="Appointments">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="CalendarWeekButton" layoutX="447.0" layoutY="70.0" mnemonicParsing="false" onAction="#CalendarWeekButtonHandler" text="This Week" />
      <Button fx:id="CalendarMonthButton" layoutX="528.0" layoutY="70.0" mnemonicParsing="false" onAction="#CalendarMonthButtonHandler" text="This Month" />
